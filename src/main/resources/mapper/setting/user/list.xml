<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dyna.modules.setting.mapper.UserMapper">
    <select id="list"  resultType="com.dyna.modules.setting.domain.entity.User">
        SELECT id,
        username,
        password,
        realname,
        phone,
        email,
        status,
        created_at,
        created_by,
        updated_at,
        updated_by
        FROM setting_user
    </select>
</mapper>